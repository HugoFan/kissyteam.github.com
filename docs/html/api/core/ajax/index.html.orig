<span class="c1">// 全局事件</span>
<span class="nx">IO</span><span class="p">.</span><span class="nx">on</span><span class="p">(</span><span class="s1">&#39;start&#39;</span><span class="p">,</span> <span class="kd">function</span><span class="p">()</span> <span class="p">{</span>
    <span class="c1">//ajax 请求开始事件</span>
<span class="p">});</span>
<span class="nx">IO</span><span class="p">.</span><span class="nx">on</span><span class="p">(</span><span class="s1">&#39;complete&#39;</span><span class="p">,</span> <span class="kd">function</span><span class="p">()</span> <span class="p">{</span>
    <span class="c1">//ajax 请求结束事件</span>
<span class="p">});</span>
</pre></div>
</div>
</dd></dl>

